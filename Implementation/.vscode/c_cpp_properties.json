{
    "version": 4,
    "configurations": [
        {
            "name": "sam3x8e",
            "includePath": [
                "${workspaceFolder}/**",
                "../Include/**",
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${workspaceFolder}/**",
                    "../Include/**",
                    "../Source/**",
                    "../Linker/**",
                ]
            },
            "defines": [
                "__SAM3X8E__"
            ],
            "cStandard": "c11",
            "cppStandard": "c++11",
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "compilerArgs": [
                "-mcpu=cortex-m3"
            ]
        }
    ]
}